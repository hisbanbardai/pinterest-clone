{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "headers": [
    {
      "key": "Access-Control-Allow-Origin",
      "value": "https://pinterest-clone-frontend-v1.vercel.app"
    },
    { "key": "Access-Control-Allow-Credentials", "value": "true" }
  ]
}
